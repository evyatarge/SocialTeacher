{
  "name": "socialteacher",
  "version": "1.0.0",
  "description": "Teacher Social-Asistant",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "test": "test",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Social Teacher Assistant\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Evyatar Gerstle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evyatarge/SocialTeacher"
  },
  "dependencies": {
    "csv-loader": "^3.0.2",
    "electron": "^7.0.0",
    "papaparse": "^5.1.0"
  },
  "devDependencies": {
    "electron-packager": "^14.1.0"
  }
}
